/** 
 * Multiplexor:
 * if (sel == 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=a, b=sel, out=nandsela);
    Nand(a=sel,b=sel,out=notsel);
    Nand(a=notsel,b=b=b,out=nandselb);
    Nand(a=nandsela,b=nandselb,out=out)
}
